{
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/node": "13.1.8",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "eslint-plugin-sort-class-members": "1.6.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.0",
    "jest": "25.1.0",
    "node-dev": "4.0.0",
    "prettier": "1.19.1",
    "ts-jest": "24.3.0",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn exam",
      "pre-push": "yarn test"
    }
  },
  "license": "MIT",
  "name": "empl-interviews",
  "scripts": {
    "compile": "tsc",
    "exam": "yarn lint && yarn compile",
    "lint": "eslint --ext ts ./src",
    "play": "tsnd --clear --log-error --no-notify --prefer-ts --pretty --respawn",
    "start": "yarn play --project playground/playground.tsconfig.json playground/playground.ts",
    "test": "jest"
  },
  "version": "0.1.0"
}
