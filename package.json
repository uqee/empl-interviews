{
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "node-dev": "7.2.0",
    "prettier": "2.6.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn exam",
      "pre-push": "yarn test"
    }
  },
  "license": "MIT",
  "name": "empl-interviews",
  "scripts": {
    "compile": "tsc",
    "exam": "yarn lint && yarn compile",
    "lint": "eslint --ext ts ./src",
    "play": "tsnd --clear --log-error --no-notify --prefer-ts --pretty --respawn",
    "postinstall": "husky install",
    "start": "yarn play --project playground/playground.tsconfig.json playground/playground.ts",
    "test": "jest"
  },
  "version": "0.1.0"
}
