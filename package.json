{
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "eslint": "6.5.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-sort-class-members": "1.6.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "node-dev": "4.0.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "ts-node-dev": "1.0.0-pre.43",
    "typescript": "3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn exam",
      "pre-push": "yarn test"
    }
  },
  "license": "MIT",
  "name": "empl-interviews",
  "scripts": {
    "compile": "tsc",
    "exam": "yarn lint && yarn compile",
    "lint": "eslint --ext ts ./src",
    "play": "tsnd --clear --log-error --no-notify --prefer-ts --pretty --respawn",
    "start": "yarn play --project playground/playground.tsconfig.json playground/playground.ts",
    "test": "jest"
  },
  "version": "0.1.0"
}